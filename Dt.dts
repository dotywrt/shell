/dts-v1/;

#include "ipq8074.dtsi"
#include "ipq8074-hk-cpu.dtsi"
#include "ipq8074-ess.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "Sagemcom Fast 5866T";
	compatible = "sagemcom,5866t", "qcom,ipq8074";

	aliases {
		led-boot = &led_power_green;
		led-failsafe = &led_power_red;
		led-running = &led_power_green;
		led-upgrade = &led_power_red;
		serial0 = &blsp1_uart5;
		ethernet0 = &dp4;
		ethernet1 = &dp5;
		ethernet2 = &dp6;
		label-mac-device = &dp4;
	};

	chosen {
		bootargs = "console=ttyMSM0,115200n8 root=PARTUUID=875f6913-71e4-0510-ff39-ca49868b1762 gpt rootwait quiet loglevel=0 swiotlb=1 coherent_pool=2M";
		stdout-path = "serial0";
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&tlmm 44 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&tlmm 50 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_power_green: green_power {
			label = "green:power";
			gpios = <&tlmm 66 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		led_power_red: red_power {
			label = "red:power";
			gpios = <&tlmm 62 GPIO_ACTIVE_HIGH>;
		};

		led_wifi: green_wifi {
			label = "green:wifi";
			gpios = <&tlmm 19 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "netdev";
			default-state = "off";
		};

		led_signal1 {
			label = "signal:1";
			gpios = <&tlmm 43 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		led_signal2 {
			label = "signal:2";
			gpios = <&tlmm 52 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		led_signal3 {
			label = "signal:3";
			gpios = <&tlmm 53 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		led_signal4 {
			label = "signal:4";
			gpios = <&tlmm 63 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		led_signal5 {
			label = "signal:5";
			gpios = <&tlmm 64 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};

	usb_vbus: regulator-usb-vbus {
		compatible = "regulator-fixed";
		regulator-name = "usb_vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&tlmm 9 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-boot-on;
		regulator-always-on;
	};
};

&tlmm {
	mdio_pins: mdio-pins {
		mdc {
			pins = "gpio68";
			function = "mdc";
			drive-strength = <8>;
			bias-pull-up;
		};

		mdio {
			pins = "gpio69";
			function = "mdio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};

	button_pins: button-pins {
		wps_button {
			pins = "gpio44";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-up;
		};

		reset_button {
			pins = "gpio50";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};

	led_pins: led-pins {
		power_green {
			pins = "gpio66";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		power_red {
			pins = "gpio62";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		wifi {
			pins = "gpio19";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		signal1 {
			pins = "gpio43";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		signal2 {
			pins = "gpio52";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		signal3 {
			pins = "gpio53";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		signal4 {
			pins = "gpio63";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};

		signal5 {
			pins = "gpio64";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-down;
		};
	};
};

&blsp1_uart5 {
	status = "okay";
};

&sdhc_1 {
	status = "okay";

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "0:sbl1";
			reg = <0x0 0x80000>;
			read-only;
		};

		partition@80000 {
			label = "0:mibib";
			reg = <0x80000 0x80000>;
			read-only;
		};

		partition@100000 {
			label = "0:bootconfig";
			reg = <0x100000 0x40000>;
			read-only;
		};

		partition@140000 {
			label = "0:bootconfig1";
			reg = <0x140000 0x40000>;
			read-only;
		};

		partition@180000 {
			label = "0:qsee";
			reg = <0x180000 0x180000>;
			read-only;
		};

		partition@300000 {
			label = "0:qsee_1";
			reg = <0x300000 0x180000>;
			read-only;
		};

		partition@480000 {
			label = "0:devcfg";
			reg = <0x480000 0x40000>;
			read-only;
		};

		partition@4c0000 {
			label = "0:devcfg_1";
			reg = <0x4c0000 0x40000>;
			read-only;
		};

		partition@500000 {
			label = "0:apdp";
			reg = <0x500000 0x40000>;
			read-only;
		};

		partition@540000 {
			label = "0:apdp_1";
			reg = <0x540000 0x40000>;
			read-only;
		};

		partition@580000 {
			label = "0:rpm";
			reg = <0x580000 0x40000>;
			read-only;
		};

		partition@5c0000 {
			label = "0:rpm_1";
			reg = <0x5c0000 0x40000>;
			read-only;
		};

		partition@600000 {
			label = "0:cdt";
			reg = <0x600000 0x40000>;
			read-only;
		};

		partition@640000 {
			label = "0:cdt_1";
			reg = <0x640000 0x40000>;
			read-only;
		};

		partition@680000 {
			label = "0:appsblenv";
			reg = <0x680000 0x40000>;
		};

		partition@6c0000 {
			label = "0:appsbl";
			reg = <0x6c0000 0x100000>;
			read-only;
		};

		partition@7c0000 {
			label = "0:appsbl_1";
			reg = <0x7c0000 0x100000>;
			read-only;
		};

		partition@8c0000 {
			label = "0:art";
			reg = <0x8c0000 0x40000>;
			read-only;
		};

		partition@900000 {
			label = "0:hlos";
			reg = <0x900000 0x1400000>;
		};

		partition@1d00000 {
			label = "rootfs";
			reg = <0x1d00000 0x4b00000>;
		};

		partition@6800000 {
			label = "rootfs_1";
			reg = <0x6800000 0x4b00000>;
		};

		partition@b300000 {
			label = "0:wififw";
			reg = <0xb300000 0x400000>;
			read-only;
		};

		partition@b700000 {
			label = "0:ethphyfw";
			reg = <0xb700000 0x80000>;
			read-only;
		};
	};
};

&usb_0 {
	status = "okay";
};

&usb_1 {
	status = "okay";
};

&dwc_0 {
	status = "okay";
	dr_mode = "peripheral";
};

&dwc_1 {
	status = "okay";
	dr_mode = "host";
};

&qusb_phy_0 {
	status = "okay";
};

&qusb_phy_1 {
	status = "okay";
};

&ssphy_0 {
	status = "okay";
};

&ssphy_1 {
	status = "okay";
};

&mdio {
	status = "okay";

	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 37 GPIO_ACTIVE_LOW>;

	ethernet-phy-package@0 {
		compatible = "qcom,qca8075-package";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0>;

		qcom,package-mode = "qsgmii";

		qca8075_3: ethernet-phy@3 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <3>;
		};

		qca8075_4: ethernet-phy@4 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <4>;
		};
	};

	qca8081: ethernet-phy@24 {
		compatible = "ethernet-phy-id004d.d101";
		reg = <24>;
		reset-deassert-us = <10000>;
	};
};

&switch {
	status = "okay";

	switch_lan_bmp = <(ESS_PORT4 | ESS_PORT5)>;
	switch_wan_bmp = <ESS_PORT6>;
	switch_mac_mode = <MAC_MODE_QSGMII>;

	qcom,port_phyinfo {
		port@4 {
			port_id = <4>;
			phy_address = <3>;
		};
		port@5 {
			port_id = <5>;
			phy_address = <4>;
		};
		port@6 {
			port_id = <6>;
			phy_address = <24>;
			port_mac_sel = "QGMAC_PORT";
		};
	};
};

&edma {
	status = "okay";
};

&dp4 {
	status = "okay";
	phy-mode = "qsgmii";
	phy-handle = <&qca8075_3>;
	label = "lan1";
};

&dp5 {
	status = "okay";
	phy-mode = "qsgmii";
	phy-handle = <&qca8075_4>;
	label = "lan2";
};

&dp6 {
	status = "okay";
	phy-handle = <&qca8081>;
	label = "wan";
};

&wifi {
	status = "disabled";
};
